<html>
   <head>
        <script src="/js/poker.js"></script>
        <link rel="stylesheet" type="text/css" href="css/poker.css">
    </head>
    <form method="post" action="/poker">
    <center>
    <font color="pink"><h2>Welcome to DeMo's Poker</h2></font>
    <body>
        <div class="winnerbox">
        <%game = Deck.new%>
        <%y = Hand.new%>
        <%game_result = game.deal_hand(@cards).game_play()%>
        <%if game_result == "player 1 wins!" %>
        <font color="yellow">Player 1 Won!</font>
        <br><font color="yellow"><%=game.ranks(y.player1)%></font>
        <%elsif game_result == "player 2 wins!"%>
        <font color="yellow">Player 2 Won!</font>
        <br><font color="yellow"><%=game.ranks(y.player2)%></font>
        <%elsif game_result == "it's a tie!"%>
        <font color="yellow">it's a tie!</font>
        <%end%>
        </div><br><br>

        <div class="tentacle1" name="tent1">
        
        </div>
        <input type="submit" id="poker" value="Play Again">
        <div class="numbers1">
            <h2>Player1</h2>
            <%game.player1.cards.each_with_index do |v, index|%>
                <div id="<%=index%>">
                <img class="card" width="80" height="100" src="/images/<%=v.value%>.png">
                </div>
            <%end%>
        </div>
        <div class="pokerfordums">
        <h3><a href="http://www.bluffnakedpoker.com/PDF/Poker%20for%20Dummies.pdf">Link to Poker for Dummies</a></h3>
        </div>
        <div class="tentacle2">
        </div>
        <div class="numbers2">
         <h2>Player2</h2>
            <%game.player2.cards.each_with_index do |v, index1|%>
                <div id="<%=index1%>">
                <img class="card" width="80" height="100" src="/images/<%=v.value%>.png">
           <%end%>
        </div>
    <style type="text/css">body { cursor: url('data:image/x-icon;base64,'), auto; }</style>
    </form>
    <div class="cbook">
     <a href="https://www.globalgreyebooks.com/call-of-cthulhu-ebook/">Link to Call of Cthulhu E-book</a>
     </div>
     </center>
   </body>
</html>