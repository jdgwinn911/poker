<html>
   <head>
        <script src="/js/poker.js"></script>
        <link rel="stylesheet" type="text/css" href="css/poker.css">
    </head>
    <form method="post" action="/poker">
    <center>
    <font color="pink"><h2>Welcome to DeMo's Poker</h2></font>
    <body>
        <div class="winnerbox">
        <%game = Hand.new%>

        <%game_result = game.output()%>
        <br><font color="yellow"><%=game.output()%></font>
        
        </div><br><br>

        <div class="tentacle1" name="tent1">
        
        </div>
        
        <button class="button" id="poker">Play Again</button>
        <div class="numbers1">
            <h2>Player1</h2>
            <%game.player1.cards.each_with_index do |v, index|%>
                <div id="<%=index%>">
                <img class="card" width="80" height="100" src="/images/<%=v.value%><%=v.suit%>.png">
                </div>
            <%end%>
        </div>
        <div class="pokerfordums">
        <h4><a href="http://www.bluffnakedpoker.com/PDF/Poker%20for%20Dummies.pdf">Link to Poker for Dummies</a></h4>
        </div>
        <div class="tentacle2">
        </div>
        <div class="numbers2">
         <h2>Player2</h2>
            <%game.player2.cards.each_with_index do |v, index1|%>
                <div id="<%=index1%>">
                <img class="card" width="80" height="100" src="/images/<%=v.value%><%=v.suit%>.png">
           <%end%>
        </div>

    <style type="text/css">body { cursor: url('data:image/x-icon;base64,'), auto; }</style>
    </form>
     </center>
   </body>
</html>